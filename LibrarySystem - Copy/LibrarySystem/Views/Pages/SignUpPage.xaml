<Page x:Class="LibrarySystem.Views.Pages.SignUpPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LibrarySystem.Views.Pages"
      xmlns:vm="clr-namespace:LibrarySystem.ViewModels.Pages"
      xmlns:p="clr-namespace:LibrarySystem.Views.UserControls"
      xmlns:s="clr-namespace:LibrarySystem.Services"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SignUpPage">
    <Page.DataContext>
        <vm:LoginViewModel/>
    </Page.DataContext>
    <Grid>
        <Grid x:Name="MainGrid"  Grid.Row="1" Grid.Column="1">
            <p:SignupForm></p:SignupForm>
        </Grid>
    </Grid>
</Page>
